<div>
    <h1>Welcome!</h1>
    <table class="table">
        <tr>
            <th>FIRST NAME</th>
            <td>{{first_name}}</td>
        </tr>
        <tr>
            <th>LAST NAME</th>
            <td>{{last_name}}</td>
        </tr>
        <tr>
            <th>EMAIL</th>
            <td>{{email}}</td>
        </tr>
        <tr>
            <th>YEAR</th>
            <td>{{year}}</td>
        </tr>
        <tr>
            <th>MAJOR</th>
            <td>{{major}}</td>
        </tr>
        <tr>
            <th>DEGREE</th>
            <td>{{degree}}</td>
        </tr>
    </table>
</div>