{{> message}}

<div class="container">
    {{#each events}}
    <div class="row">
        <div class="col">
            <div class="card" id="event-card-4">
                Event Name: Study Session for CSCI {{this.course_no}} <br>
                Date: {{this.date}} <br>
                Start Time: {{this.start_time}} <br>
                End Time: {{this.end_time}} <br>
                Location: {{this.location}} <br>
            </div>
        </div>
        <div class="col">
            <a class="btn btn-primary" href="">Add to your Calendar!</a>
        </div>
        <br>
    </div>
    
    {{/each}}
</div>